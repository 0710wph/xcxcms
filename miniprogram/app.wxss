/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

image{border:0;margin:0;padding:0;vertical-align:middle;}.cl{clear:both;}.fl{float:left;}.fr{float:right;}.m-auto{margin:0 auto;}
.mb10{margin-bottom:10px;}.mt10{margin-top:10px;}
.box{margin-left:10px;margin-right:10px;overflow:hidden;}
.content{font-size:16px;line-height:26px;}.content text{display:block;margin-bottom:1.35em;}.content u{color:red;text-decoration:none;}.content imgage{width:100%;display:block;height:auto;}
.tit{font-size:22px;padding:10px;}.tith{font-weight:normal;font-size:22px;padding:10px;}
.tit2,.dtit{font-size:20px;font-weight:700;background-color:#dfefd8;height:43px;color:#333;line-height:43px;padding-left:5px;border-top:solid 1px rgba(101,153,1, 0.4);}




